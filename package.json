    "@typescript-eslint/parser": "^5.48.0",
    "@vitest/coverage-c8": "^0.27.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "git-commit-msg-linter": "^4.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "tsc-alias": "^1.8.2",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
